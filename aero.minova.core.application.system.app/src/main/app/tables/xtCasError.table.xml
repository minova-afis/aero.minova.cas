<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<table name="xtcasError" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="table.xsd">
	<column name="KeyLong">
		<integer nullable="false" identity="true"/>
	</column>
	<column name="Username">
		<varchar length="10" nullable="false"/>
	</column>
	<column name="ErrorMessage">
		<varchar length="250" nullable="false"/>
	</column>
	<column name="Date" default="getdate()">
		<datetime nullable="false"/>
	</column>
	<primarykey>
		<column>KeyLong</column>
	</primarykey>
</table>